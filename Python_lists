myfile = open('listfile.txt')
#print(myfile)
print(myfile.read())
print(myfile.read())      #o/p:- ' ' [this space is due to the cursor goes from starting to end so to start again you need to seek the cursor to the starting point(seek(0))]

myfile.seek(0)
print(myfile.read())

myfile.seek(0)
print(myfile.readlines())  #look the lines through list
myfile.close()
